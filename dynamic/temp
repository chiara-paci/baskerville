CREATE TABLE "bibliography_bookauthorrelation" (
    "authorrelation_ptr_id" integer NOT NULL PRIMARY KEY REFERENCES "bibliography_authorrelation" ("id") DEFERRABLE INITIALLY DEFERRED,
    "pos" integer CHECK ("pos" >= 0) NOT NULL,
    "book_id" integer NOT NULL REFERENCES "bibliography_book" ("id") DEFERRABLE INITIALLY DEFERRED
)
;
--
CREATE INDEX "bibliography_bookauthorrelation_book_id" ON "bibliography_bookauthorrelation" ("book_id");
CREATE INDEX "bibliography_textscdrom_books_textscdrom_id" ON "bibliography_textscdrom_books" ("textscdrom_id");
CREATE INDEX "bibliography_textscdrom_books_book_id" ON "bibliography_textscdrom_books" ("book_id");
CREATE INDEX "bibliography_textscdrom_label_like" ON "bibliography_textscdrom" ("label" varchar_pattern_ops);
CREATE INDEX "bibliography_bookseriewithoutisbn_isbn_ced" ON "bibliography_bookseriewithoutisbn" ("isbn_ced");
CREATE INDEX "bibliography_bookseriewithoutisbn_isbn_ced_like" ON "bibliography_bookseriewithoutisbn" ("isbn_ced" varchar_pattern_ops);
